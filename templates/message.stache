<div class="mainpost" msgid="{{MessageID}}">
    <div class="commentprofileimg"><img src="{{pic}}" alt="avatar" name="userImage" /></div>
    <div class="commentdisplayname msglink" userid="{{LinkID}}">
      <h3>{{displayname}}</h3></div>
    <abbr class="posttime timeago">{{Time}}</abbr>
    <div class="postcontent">{{Content}}</div>
</div>            
