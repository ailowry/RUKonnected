<div class="mainpost" msgid="{{MessageID}}">
  <table>
    <tr>
      <td class="commentprofileimg">
        <img src="{{pic}}" alt="avatar" name="userImage" />
      </td>
      <td class="commentdisplayname msglink" userid="{{LinkID}}">
        <a><h3>{{displayname}}</h3></a>
      </td>
      <td class="commenttime">
        <em>{{Time}}</em>
      </td>
    </tr>
    <tr>
      <td colspan="4" class="commentcontent">
        {{Content}}
      </td>
    </tr>
  </table>
</div>
